<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="FirefoxSuite" parallel="tests" thread-count="1">
    <listeners>
        <listener class-name="hooks.suitelistener.SuiteListener"/>
    </listeners>
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>

    <test name="RunOnFirefox">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="runner.googlerunner.GoogleRunner"/>
        </classes>
    </test>

</suite>
